{{#extends 'base'}}


{{#block "content"}}

<h1>Deleting variant from store</h1>


<form method="POST">
    <input type="hidden" name="_csrf" value="{{csrfToken}}" />
    <div class="alert alert-danger"> Please confirm to delete {{variant.name}}?</div>
    <input type="submit" class="btn btn-danger" value="Delete" />
    <a href="/products" class="btn btn-success">Cancel</a>

</form>


{{/block}}

{{/extends}}